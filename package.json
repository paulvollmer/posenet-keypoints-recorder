{
  "name": "posenet-keypoints-recorder",
  "version": "0.1.1",
  "description": "posenet keypoints recorder",
  "main": "server.js",
  "scripts": {
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "start": "node server.js",
    "watch": "NODE_ENV=development parcel --open index.html ",
    "build": "NODE_ENV=production parcel build index.html --public-url ./",
    "clean": "rm -rf .cache dist"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:paulvollmer/posenet-keypoints-recorder.git"
  },
  "keywords": [
    "posenet",
    "keypoints",
    "recorder"
  ],
  "author": "paulvollmer",
  "license": "Apache-2.0",
  "dependencies": {
    "@tensorflow-models/posenet": "^0.2.2",
    "@tensorflow/tfjs": "^0.12.4",
    "dat.gui": "^0.7.2",
    "posenet-keypoints-viewer": "paulvollmer/posenet-keypoints-viewer#0.2.0",
    "stats.js": "^0.17.0"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-polyfill": "~6.26.0",
    "babel-preset-env": "~1.6.1",
    "babel-preset-es2017": "^6.24.1",
    "clang-format": "~1.2.2",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "parcel-bundler": "~1.6.2"
  },
  "eslintConfig": {
    "extends": "google",
    "env": {
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    }
  },
  "eslintIgnore": []
}
